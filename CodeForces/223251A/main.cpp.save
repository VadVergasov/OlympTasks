#include <bits/stdc++.h>

using namespace std;

int main(){
    int n, q, tmp;
    cin>>n;
    vector<int> cost;
    for(int i=0;i<n;i++){
        cin>>tmp;
        cost.push_back(tmp);
    }
    sort(cost.begin(), cost.end());
    cin>>q;
    for(int i=0;i<q;i++){
        cin>>tmp;
        cout<<(upper_bound(cost.begin(), cost.end(), tmp-1)-cost.begin()<<"\n";
    }
    return 0;
}
